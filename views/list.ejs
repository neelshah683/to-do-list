<%- include("./header") -%>

    <div class="box" id="heading">
      <h1>
        <%= listTitle %></h1>
    </div>

    <div class="box">
      
        <% 
            for(var i = 0; i<listItems.length;i++)
            {
        %>
              <div class="item">
                <input type="checkbox">
                <p> <%= listItems[i] %></p>    
              </div>
        <%
            }
        %> 
        <form class="item" action="/" method="POST">
      <input
        type="text"
        name="listno"
        placeholder="Add New Item"
        autocomplete="off"
      /><br/><br/>
      <button name="list" type="submit" value= <%=listTitle%> >+</button>
    </form>
    </div> 

<%-include ("./footer") -%>
